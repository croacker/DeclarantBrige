<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DeclarantBrige" namespace="com.asf.declarantbrige.model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="RefTypeProduce" table="ref_type_produce">
    <id name="Id" type="Int32">
      <column name="id" not-null="true" precision="10" sql-type="int" />
      <generator class="identity" />
    </id>
    <property name="Vid" type="String">
      <column name="vid" not-null="true" length="10" sql-type="nvarchar" />
    </property>
    <property name="Descr" type="String">
      <column name="descr" not-null="true" length="50" sql-type="nvarchar" />
    </property>
  </class>
</hibernate-mapping>