<?xml version="1.0" encoding="utf-8"?>
<hibernate-mapping assembly="DeclarantBrige" namespace="com.asf.declarantbrige.model" xmlns="urn:nhibernate-mapping-2.2">
  <class name="SysPinfo" table="sys_pinfo">
    <id name="Id" type="Int32">
      <column name="id" not-null="true" precision="10" sql-type="int" unique="true" />
      <generator class="identity" />
    </id>
    <property name="InfoCode" type="Int32">
      <column name="info_code" not-null="true" precision="10" sql-type="int" unique="true" />
    </property>
    <property name="InfoValue" type="String">
      <column name="info_value" not-null="false" length="255" sql-type="nvarchar" />
    </property>
  </class>
</hibernate-mapping>